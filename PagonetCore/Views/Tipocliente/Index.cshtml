@model IEnumerable<PagonetCore.Models.Adtipo_cliente>

@{
    ViewBag.Title = "Tipos de Cliente";
}

<h2>Tipos de Cliente</h2>

<p>
    @Html.ActionLink("Crear", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.tip_cli)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.des_tipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.importado_web)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.importado_pro)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tip_cli)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.des_tipo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.importado_web)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.importado_pro)
        </td>
        <td>
            @Html.ActionLink("Modificar", "Edit", new { id=item.id_tipocliente }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.id_tipocliente }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.id_tipocliente })
        </td>
    </tr>
}

</table>
