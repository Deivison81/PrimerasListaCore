@model PagonetCore.Models.Adpais

@{
    ViewBag.Title = "Detalles de País";
}

<h2 class="d-flex justify-content-center">Detalles de País</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.co_pais)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.co_pais)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.pais_des)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.pais_des)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.importado_web)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.importado_web)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.importado_pro)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.importado_pro)
        </dd>

    </dl>
</div>
<p class="d-flex justify-content-center">
    @Html.ActionLink("Modificar", "Edit", new { id = Model.id_pais }) &nbsp;|&nbsp;
    @Html.ActionLink("Volver a Países", "Index")
</p>
