@model PagonetCore.Models.AdBanco

@{
    ViewBag.Title = "Eliminar Banco";
}

<h2 class="d-flex justify-content-center">Eliminar Banco</h2>

<h3 class="d-flex justify-content-center">¿Está seguro de que desea eliminar el Banco?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.co_ban)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.co_ban)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.des_ban)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.des_ban)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.importado_web)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.importado_web)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.importado_pro)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.importado_pro)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color col-10 offset-2 col-md-7 offset-md-5">
            <input type="submit" value="Eliminar" class="btn btn-default" /> |
            @Html.ActionLink("Volver a Bancos", "Index")
        </div>
    }
</div>
