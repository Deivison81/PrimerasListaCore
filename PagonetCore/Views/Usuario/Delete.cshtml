@model PagonetCore.Models.Adusuarios

@{
    ViewBag.Title = "Eliminar Usuario";
}

<h2 class="d-flex justify-content-center">Eliminar Usuario</h2>

<h3 class="d-flex justify-content-center">¿Está seguro de que desea eliminar?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.co_user_prof)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.co_user_prof)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.cod_user)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.cod_user)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.nombre_usuarios)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.nombre_usuarios)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.password)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.password)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.Estado)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.Estado)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.fecha_ingreso)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.fecha_ingreso)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.validacion)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.validacion)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color col-10 offset-2 col-md-7 offset-md-5">
            <input type="submit" value="Eliminar" class="btn btn-default" /> &nbsp;|&nbsp;
            @Html.ActionLink("Volver", "Index")
        </div>
    }
</div>
