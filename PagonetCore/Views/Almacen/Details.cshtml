@model PagonetCore.Models.AdAlmacen

@{
    ViewBag.Title = "Detalles Almacén";
}

<h2 class="d-flex justify-content-center">Detalles Almacén</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.co_alma)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.co_alma)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.des_alamacen)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.des_alamacen)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.web)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.web)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.co_user_prof)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.co_user_prof)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.importado_web)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.importado_web)
        </dd>

        <dt class="d-flex justify-content-center">
            @Html.DisplayNameFor(model => model.importado_pro)
        </dt>

        <dd class="d-flex justify-content-center">
            @Html.DisplayFor(model => model.importado_pro)
        </dd>

    </dl>
</div>
<div class="row justify-content-center">
    <div class="col-9 offset-3 mr-5 col-sm-7 col-md-6 offset-lg-4 mr-lg-5">
        @Html.ActionLink("Modificar Almacén", "Edit", new { id = Model.cod_almacen }, new { @type = "button", @class = "form-btn btn btn-primary btn-block" })
        @Html.ActionLink("Volver a Almacenes", "Index", null, new { @type = "button", @class = "form-btn btn btn-light btn-block" })
    </div>
</div>
